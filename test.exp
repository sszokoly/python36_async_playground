# Define the variable
set var "00033     2024-11-21,09:38:23 09:38:28           G711U                           \n--type q to quit or space key to continue-- \n 10.10.48.166\n00034     2024-11-21,09:53:49 09:54:10           G711U                            10.10.48.192\n00035     2024-11-21,09:53:53 09:53:54           G711U                            10.10.48.192\n"
puts $var
# Define the pattern to remove
set pattern {\n--type q to quit or space key to continue-- \n}

# Use regsub to replace all occurrences of the pattern with an empty string
regsub -all $pattern $var "" var

# Print the modified variable
puts "Modified variable:\n$var"